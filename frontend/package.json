{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:e2e:ci": "cypress run",
    "test:e2e:login:ci": "cypress run --spec 'cypress/e2e/login.cy.ts'",
    "test:e2e:product-crud:ci": "cypress run --spec 'cypress/e2e/product-crud.cy.ts'",
    "test:e2e:product-create:ci": "cypress run --spec 'cypress/e2e/product-create.cy.ts'",
    "test:e2e:product:ci": "npm run test:e2e:product-create:ci && npm run test:e2e:product-crud:ci",
    "report:e2e:merge": "mochawesome-merge cypress/reports/*.json > cypress/reports/report.json",
    "report:e2e:html": "marge cypress/reports/report.json -f report -o cypress/reports/html",
    "report:e2e": "npm run report:e2e:merge && npm run report:e2e:html"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/vite": "^4.1.17",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.14.1",
    "tailwindcss": "^4.1.17",
    "vite-plugin-pages": "^0.33.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "cypress": "^15.6.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.0",
    "mochawesome-report-generator": "^6.3.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.13",
    "wait-on": "^9.0.3"
  }
}
